{
  "name": "template",
  "version": "1.0.0",
  "description": "A template for a client-server nengi/pixi/vue game",
  "main": "index.js",
  "scripts": {
    "client": "parcel serve src/client/ui/index.html --port=4040",
    "server": "tsc-watch --onSuccess \"node ./out/server/main.js\"",
    "start": "concurrently \"npm:server\" \"npm:client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "author": "angus fletcher",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.10.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "less": "^3.12.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "prettier": "^2.1.1",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2",
    "vue-template-compiler": "^2.6.12"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "dependencies": {
    "nengi": "^1.17.0",
    "pixi.js": "^5.3.3",
    "ts-vue-plugin": "^0.1.3",
    "vue": "^2.6.12",
    "vue-hot-reload-api": "^2.3.4",
    "vuex": "^3.5.1"
  },
  "staticFiles": {
    "staticPath": "assets",
    "watcherGlob": "**"
  }
}
